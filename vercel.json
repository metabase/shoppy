{
  "version": 2,
  "git": {
    "deploymentEnabled": {
      "*": false,
      "shoppy-metabot": true
    }
  },
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=2592000"
        }
      ]
    },
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=2592000"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=2592000"
        }
      ]
    },
    {
      "source": "/mb/app/embedding-sdk.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=0, s-maxage=2592000"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/src/main"
    },
    {
      "source": "/mb/:path*",
      "destination": "https://shoppy.coredev.metabase.com/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
