{
  "version": 2,
  "git": {
    "deploymentEnabled": {
      "*": false,
      "shoppy-metabot": true
    }
  },
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=2592000"
        }
      ]
    },
    {
      "source": "/fonts/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=2592000"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=2592000"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/sso/metabase",
      "destination": "https://shoppy-sdk-auth.kelvin.workers.dev"
    },
    {
      "source": "/api/:path*",
      "destination": "/api/src/main"
    },
    {
      "source": "/mb/:path*",
      "destination": "https://shoppy.coredev.metabase.com"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
